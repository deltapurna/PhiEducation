<h1>Welcome to Phi Education</h1>
<%= form_for @room do |f| %>
  <%= f.fields_for :questions do |q| %>
    <%= render 'questions/form', f:q %>
  <% end %>
  <p><%= f.submit 'Create your classroom!' %></p>
<% end %>

<h2>Already have one? <%= link_to 'Go to your classroom', new_teacher_path %></h2>
<h2>Student? <%= link_to 'Click here', new_student_path %></h2>
